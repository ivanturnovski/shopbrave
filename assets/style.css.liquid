.loader-holder {
	width: 100%;
	height: 100vh;
	display: flex;
	justify-content: center;
	align-items: center;
	background: #6a6a48;
	position: fixed;
	z-index: 999;
}
.loader-holder .loader {
	border: 5px solid #f3f3f3;
	border-radius: 50%;
	border-top: 5px solid #6a6a48;
	border-bottom: 5px solid #6a6a48;
	width: 50px;
	display: flex;
	height: 50px;
	-webkit-animation: spin 2s linear infinite;
	animation: spin 2s linear infinite;
	z-index: 999;
	top: 0;
	left: 0;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}

@-webkit-keyframes spin {
	0% {
		-webkit-transform: rotate(0deg);
	}
	100% {
		-webkit-transform: rotate(360deg);
	}
}
@keyframes spin {
	0% {
		transform: rotate(0deg);
	}
	100% {
		transform: rotate(360deg);
	}
}
